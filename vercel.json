{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/" }],
  "install": {
    "include": ["package.json", "package-lock.json"],
    "npmClient": "npm",
    "args": ["install", "--production"]
  }
}
